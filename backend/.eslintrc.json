{
  "extends": [
    "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "prefer-const": "error",
    "no-var": "error",
    "no-console": "off"
  },
  "env": {
    "node": true,
    "es2020": true
  }
}